{
  "name": "chart-rendering-fa",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "tsc && tsc-alias",
    "build:ci": "npm run prestart && npm run publish",
    "watch": "tsc -w",
    "clean": "rimraf dist && rimraf out",
    "prestart": "npm run clean && npm run build",
    "publish": "copyfiles *.json -e ts*.json out && copyfiles -u 1 \"dist/**/*\" out/dist && copyfiles -u 1 \"node_modules/**/*\" out/node_modules",
    "dev": "npm run api && tsc --project tsconfig.dev.json && tsc-alias && func host start --port 7076",
    "start": "func start",
    "test": "jest",
    "test:ci": "jest --coverage --coverageDirectory=tests/out",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "api": "ts-oas ./src/openapi/index.ts /^\\w+Api$/ -o ./src/assets/openapi.json -c ./src/openapi/tsconfig.json -p ./ts-oas.json"
  },
  "dependencies": {
    "@azure/functions": "^4.7.2",
    "@xmldom/xmldom": "^0.9.8",
    "applicationinsights": "^3.9.0",
    "classnames": "^2.5.1",
    "d3": "^7.9.0",
    "query-selector": "^2.0.0",
    "sprintf-js": "^1.1.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@types/d3": "^7.4.3",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.3.1",
    "@types/pg": "^8.15.6",
    "@types/sprintf-js": "^1.1.4",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "copyfiles": "^2.4.1",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "jest": "^30.1.2",
    "jest-junit": "^16.0.0",
    "jest-theories": "^1.5.1",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "swagger-ui-dist": "^5.28.0",
    "ts-jest": "^29.4.4",
    "ts-oas": "0.5.2",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  },
  "main": "dist/src/{index.js,functions/**/route.js}",
  "type": "module",
  "scarfSettings": {
    "enabled": false
  },
  "engines": {
    "node": ">=22"
  }
}