{
  "FeatureManagement": {
    "HighExecutivePay": true,
    "HighNeeds": true,
    "SchoolSpendingPrioritiesSsrCharts": true,
    "CfrItSpendBreakdown": true,
    "News": true,
    "BfrItSpendBreakdown": true
  },
  "Apis": {
    "Insight": {
      "Url": "http://localhost:7071/",
      "Key": "x"
    },
    "Benchmark": {
      "Url": "http://localhost:7072/",
      "Key": "x"
    },
    "Establishment": {
      "Url": "http://localhost:7073/",
      "Key": "x"
    },
    "LocalAuthorityFinances": {
      "Url": "http://localhost:7074/",
      "Key": "x"
    },
    "NonFinancial": {
      "Url": "http://localhost:7075/",
      "Key": "x"
    },
    "ChartRendering": {
      "Url": "http://localhost:7076/",
      "Key": "x"
    },
    "Content": {
      "Url": "http://localhost:7077/",
      "Key": "x"
    }
  },
  "WebAssets": {
    "FilesBaseUrl": "http://127.0.0.1:10000/devstoreaccount1/files",
    "ImagesBaseUrl": "http://127.0.0.1:10000/devstoreaccount1/images"
  },
  "CosmosCacheSettings": {
    "ContainerName": "sessions",
    "DatabaseName": "session-data"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.ApplicationInsights"
    ],
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "ApplicationInsights",
        "Args": {
          "telemetryConverter": "Serilog.Sinks.ApplicationInsights.TelemetryConverters.TraceTelemetryConverter, Serilog.Sinks.ApplicationInsights",
          "restrictedToMinimumLevel": "Warning"
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithThreadId",
      "WithMachineName",
      "WithCorrelationIdHeader"
    ],
    "Properties": {
      "Application": "Financial Benchmarking and Insights Tool"
    }
  },
  "DISABLE_ORG_CLAIM_CHECK": true
}