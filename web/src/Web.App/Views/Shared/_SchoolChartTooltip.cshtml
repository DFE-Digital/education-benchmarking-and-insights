@using Web.App.Extensions
@model Web.App.ViewModels.SchoolChartTooltipViewModel

<script type="module" add-nonce="true">
    import {createApp, SchoolChartTooltips} from "@Html.FileVersionedPath("/js/main.js")";

    const data = @Html.Raw(Json.Serialize(Model.Data));

    const container = document.createElement("div");
    const main = document.querySelector("main");
    main.appendChild(container);

    const app = createApp(SchoolChartTooltips, {data});
    const vm = app.mount(container);
</script>