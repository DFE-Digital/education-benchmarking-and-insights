@model Web.App.ViewModels.TrustHistoryViewModel

@{
    ViewData[ViewDataKeys.Title] = PageTitles.HistoricData;
}

@await Component.InvokeAsync("EstablishmentHeading", new { title = ViewData[ViewDataKeys.Title], name = Model.Name, id = Model.CompanyNumber, kind = OrganisationTypes.Trust })

<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        <p class="govuk-body">View data from previous financial years.</p>
        <p class="govuk-body">All data is from academies accounts return (AAR) return.</p>
    </div>
</div>

<div id="historic-data" data-type="@OrganisationTypes.Trust" data-id="@Model.CompanyNumber"></div>