@model Web.App.ViewModels.LocalAuthorityHighNeedsViewModel
@{
    ViewData[ViewDataKeys.Title] = PageTitles.LocalAuthorityHighNeedsNationalRankings;
    var chartTitle = $"National view {Model.Section251Year - 1}-{Model.Section251Year}";
}

@await Component.InvokeAsync("EstablishmentHeading", new
{
    title = ViewData[ViewDataKeys.Title],
    name = Model.Name,
    id = Model.Code,
    kind = OrganisationTypes.LocalAuthority
})

@await Component.InvokeAsync("DataSource", new
{
organisationType = OrganisationTypes.LocalAuthority,
sourceType = DataSourceTypes.HighNeeds,
additionText = new[] { "This data shows local authorities ranked based on budget vs outturn, funding vs outturn is not available." },
pageTitle = ViewData[ViewDataKeys.Title]
})

<div id="la-national-rank" data-code="@Model.Code" data-title="@chartTitle"></div>