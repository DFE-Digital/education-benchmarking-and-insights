@model Web.App.ViewModels.SchoolCensusViewModel
@{
    ViewData[ViewDataKeys.Title] = PageTitles.Census;
}

@await Component.InvokeAsync("EstablishmentHeading", new
    {
        title = PageTitles.Census,
        name = Model.Name,
        id = Model.Urn,
        kind = OrganisationTypes.School
    })

@await Component.InvokeAsync("CustomDataBanner", new
    {
        name = Model.Name,
        id = Model.Urn,
    })

<div id="compare-your-census" data-type="@OrganisationTypes.School" data-id="@Model.Urn" data-custom-data-id="@Model.CustomDataId"></div>

@await Html.PartialAsync("FinanceTools/_SchoolCustom", Model.CustomTools)

